<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent">
    <LinearLayout
        android:layout_width="@dimen/ymcontent_width"
        android:layout_height="@dimen/ymcontent_heigth"
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:background="@mipmap/background">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60.0dp"
            android:layout_marginLeft="10.0dp"
            android:layout_marginRight="10.0dp"
            android:layout_marginBottom="6.0dp"
            android:layout_marginTop="10.0dp">

            <ImageButton android:id="@+id/login_back"
                android:layout_width="30.0dp"
                android:layout_height="60.0dp"
                android:src="@mipmap/back"
                android:background="@android:color/transparent"/>

        </RelativeLayout>
    </LinearLayout>


</RelativeLayout>